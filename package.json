{
  "name": "quickie",
  "version": "1.0.0",
  "description": "quickie boilerplate",
  "scripts": {
    "test": "eslint .",
    "build": "parcel build src/index.html",
    "start": "parcel src/index.html --open"
  },
  "author": "",
  "license": "ISC",
  "package-lock": false,
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "node-sass": "^4.10.0",
    "parcel-bundler": "^1.10.3",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.15.2"
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ],
    "root": true,
    "env": {
      "browser": true,
      "es6": true
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "postcss": {
    "modules": true
  },
  "eslintIgnore": [
    "dist"
  ],
  "dependencies": {}
}
